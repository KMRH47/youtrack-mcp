[run]
source = youtrack_mcp
omit = 
    tests/*
    venv/*
    */venv/*
    */virtualenv/*
    */__pycache__/*
    */migrations/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = YouTrack MCP Coverage Report

[coverage:paths]
source =
    youtrack_mcp/
    */site-packages/youtrack_mcp/ 